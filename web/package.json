{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "//": "Remark scripts based on https://github.com/facebook/docusaurus/issues/3321#issuecomment-1477543749",
    "build": "npm run generate-llm-files && docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "generate-llm-files": "tsx ./scripts/generate-llm-files.ts",
    "remark": "remark",
    "remark:once": "npm run remark -- --quiet --frail --use remark-validate-links docs/",
    "serve": "docusaurus serve",
    "start": "DOCS_INCLUDE_CURRENT_VERSION=true docusaurus start --port 3002",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@kodingdotninja/use-tailwind-breakpoint": "^0.0.5",
    "@mdx-js/react": "^3.1.0",
    "@svgr/webpack": "^6.5.1",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "front-matter": "^4.0.2",
    "glob": "^11.0.2",
    "plugin-image-zoom": "github:flexanalytics/plugin-image-zoom",
    "postcss": "^8.4.19",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.29.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-feather": "^2.0.10",
    "react-modal": "^3.14.3",
    "react-player": "^2.16.0",
    "react-tooltip": "^4.5.1",
    "react-transition-group": "^4.4.5",
    "tailwindcss": "^3.2.4",
    "ts-blank-space": "^0.6.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/tsconfig": "^3.7.0",
    "@types/mdast": "^4.0.4",
    "docusaurus-plugin-typedoc": "^1.4.2",
    "mdast": "^2.3.2",
    "mdast-util-mdx": "^3.0.0",
    "prettier": "^3.0.3",
    "remark-cli": "^11.0.0",
    "remark-validate-links": "^12.1.1",
    "tsx": "^4.20.3",
    "typedoc": "^0.28.14",
    "typedoc-plugin-frontmatter": "^1.3.0",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.8.3",
    "unified": "^11.0.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
