      "./client/operations": "./user-core/dist/client/operations/index.js",
      "./server/auth": "./user-core/dist/server/auth/index.js",
      "./server/crud": "./user-core/dist/server/crud/index.js",
      "./client/crud": "./user-core/dist/client/crud/index.js",
      "./server/email": "./user-core/dist/server/email/index.js",
