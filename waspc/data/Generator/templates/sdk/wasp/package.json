      {=! Private: [] =}
      "./server": "./user-core/dist/server/index.js",
      {=! Public: { type MyApiRoute1, type MyApiRoute2, ... }  =}
      {=! Private: [] =}
      "./server/api": "./user-core/dist/server/api/index.js",
      {=! Public: { api }  =}
      {=! Private: [sdk] =}
      "./client/api": "./user-core/dist/api/index.js",
