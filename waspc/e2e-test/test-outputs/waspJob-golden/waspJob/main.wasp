app waspJob {
  db: { system: PostgreSQL },
  wasp: {
    version: "^0.12.2"
  },
  title: "waspJob"
}

route RootRoute { path: "/", to: MainPage }
page MainPage {
  component: import { MainPage } from "@src/MainPage"
}
 job mySpecialJob { 
   executor: PgBoss, 
   perform: { 
     fn: import { foo } from "@src/server/jobs/bar.js"  
   } 
 } 

