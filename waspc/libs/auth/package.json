{
  "name": "@wasp.sh/libs-auth",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "types": "./dist/client/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "prepare": "npm run build",
    "test": "npm run test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "oslo": "^1.1.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.4",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.4"
  },
  "typesVersionComment": "typesVersions is needed until the SDK tsconfig.json has moduleResolution set to 'bundler'."
}
